{
  "dependencies": {
    "body-parser": "^1.20.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "ffmpeg-for-static": "^1.0.0",
    "ffmpeg-static": "^5.0.2",
    "file-type": "^17.1.2",
    "jimp": "^0.16.0",
    "libreoffice-convert": "^1.4.1",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "postinstall": "electron-builder install-app-deps",
  "devDependencies": {
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0"
  },
  "description": "A set of conversion tools",
  "author": "Andrew",
  "name": "file-convert",
  "version": "0.1.0",
  "license": "MIT",

  "build": {
    "files":[
      "main.js",
      "node_modules"
    ],
    "appId": "com.andrew.fileconvert",
    "mac":{
      "category":"Utility"
    }
    ,
    "linux": {
      "target": [
        "AppImage"
      ],
      "category":"Utility"
    },
    "win": {
      "target": "nsis"    }
  }
}
